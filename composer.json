{
  "name": "nayleen/async-console",
  "description": "Asynchronous wrappers and utilities around symfony/console",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "nayleen/async": "~0",
    "nayleen/finder": "~0",
    "symfony/console": "^6"
  },
  "require-dev": {
    "nayleen/codestandard": "dev-latest",
    "nayleen/development-dependencies": "^3",
    "roave/security-advisories": "dev-latest"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Nayleen\\Async\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nayleen\\Async\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1.x-dev"
    }
  }
}
